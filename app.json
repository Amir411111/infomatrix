{
  "expo": {
    "name": "ClothMatch",
    "slug": "clothmatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clothmatch.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.clothmatch.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Приложение нуждается в доступе к вашим фотографиям для добавления вещей в гардероб.",
          "cameraPermission": "Приложение нуждается в доступе к камере для фотографирования вещей."
        }
      ]
    ]
  }
}

